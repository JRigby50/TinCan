<html>
<head>
<title>Northrup Grumman Alternative Computing</title>
<HTA:APPLICATION
  APPLICATIONNAME="ThinClientMenu"
  ID="ThinClientMenu"
  VERSION="1.0"
  MAXIMIZEBUTTON="no"
  MINIMIZEBUTTON="no"
  SCROLL="no"
  SINGLEINSTANCE="yes"/>
</head>

<script language="VBScript">

Sub Window_OnLoad
self.Focus()
self.ResizeTo 252,200
End Sub

Sub OnClickButtonCitrix()
Set WshShell = CreateObject("WScript.Shell")
strCmdLine = "Cmd /C C:\Windows\launch.bat"
WshShell.Run strCmdLine,1,True
End Sub

Sub OnClickButtonShutdown()
Set WshShell = CreateObject("WScript.Shell")
strCmdLine = "Cmd /C shutdown  -s -f -t 0"
WshShell.Run strCmdLine,1,True
End Sub

Sub OnClickButtonRestart()
Set WshShell = CreateObject("WScript.Shell")
strCmdLine = "Cmd /C shutdown  -r -f -t 0"
WshShell.Run strCmdLine,1,True
End Sub
</script>

<body STYLE="font:10 pt arial; color:black;
 filter:progid:DXImageTransform.Microsoft.Gradient
(GradientType=0, StartColorStr='#000000', EndColorStr='#AAFFFF')"> <!--bgcolor="white"

<!--Add your controls here-->

<input type="button" name="Citrix" id="Citrix" value="Connect Citrix Desktop" onclick="OnClickButtonCitrix"><BR><BR>
<input type="button" name="Shutdown" id="Shutdown" value="Shutdown" onclick="OnClickButtonShutdown"><BR><BR>
<input type="button" name="Restart" id="Restart" value="Restart" onclick="OnClickButtonRestart">
